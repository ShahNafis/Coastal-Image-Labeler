{
  "name": "psic_node_server",
  "version": "1.0.0",
  "description": "PSIC Node server",
  "main": "server.js",
  "scripts": {
    "buildServer": "tsc",
    "buildSite" :"next build ./_site",
    "test": "tsc && node ./_dist/test.js",
    "test2": "tsc && SET NODE_ENV=production && node ./_dist/test.js",
    "dev": "nodemon --exec ts-node ./_src/server.ts",
    "devNoRestart": "ts-node ./_src/server.ts",
    "start": "tsc && SET NODE_ENV=production&& node ./_dist/server.js",
    "startNoTSC": "SET NODE_ENV=production && node ./_dist/server.js",
    "seederCreate": "ts-node ./_src/jsonSeeder.ts -i",
    "seederDel": "ts-node ./_src/jsonSeeder.ts -d",
    "seederRole": "ts-node ./_src/jsonSeeder.ts -r",
    "stormSeeder": "ts-node ./_src/stormSeeder.ts",
    "seeder": "npm run seederDel && npm run seederCreate"
  },
  "author": "Shah Nafis Rafique",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "axios": "^0.19.2",
    "clsx": "latest",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.17.1",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "next": "^9.3.5",
    "node-geocoder": "^3.27.0",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.2",
    "prop-types": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-moment": "^0.9.7",
    "slugify": "^1.4.0",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/express-fileupload": "^1.1.3",
    "@types/mongodb": "^3.5.8",
    "@types/mongoose": "^5.7.12",
    "@types/morgan": "^1.9.0",
    "@types/node": "^12.12.36",
    "@types/node-geocoder": "^3.24.1",
    "@types/passport": "^1.0.3",
    "@types/react-dom": "^16.9.6",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
